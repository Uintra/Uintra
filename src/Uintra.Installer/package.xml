      <Package name="uIntra.Core" version="1.7.0.7" />
      <Package name="uIntra.Events" version="1.7.0" />
      <Package name="uIntra.Groups" version="1.1.0" />
      <Package name="uIntra.Likes" version="1.4.0" />
      <Package name="uIntra.Navigation" version="1.4.5" />
      <Package name="uIntra.News" version="1.5.0" />
      <Package name="uIntra.Notification" version="1.5.0.3" />
