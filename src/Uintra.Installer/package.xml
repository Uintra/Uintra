      <Package name="uIntra.Users" version="1.7.0" />
    </Action>

    <Action runat="install" undo="false" alias="ExtractNugetPackages" sourceDirectory="~/umbraco/uintra/nupkgs/" targetFramework="471" />

    <Action runat="install" undo="false" alias="ApplyTransformations">
      <Config path="~/config/Lang/da-DK.user.xml" />
      <Config path="~/config/Lang/en-GB.user.xml" />
      <Config path="~/config/Lang/en-US.user.xml" />
      <Config path="~/config/Dashboard.config" />
      <Config path="~/config/applications.config" />
      <Config path="~/config/404handlers.config" />
      <Config path="~/config/ClientDependency.config" />
      <Config path="~/config/trees.config" />
      <Config path="~/config/imageprocessor/processing.config" />
      <Config path="~/Views/Web.config" pattern="Views.Web.config.install.xdt" />
      <Config path="~/Web.config" />
    </Action>

    <Action runat="install" undo="false" alias="SendXmlLogFile" uri="https://uintra.dk/umbraco/api/packagelogger/report" />

    <Action runat="install" undo="false" alias="Clear" installerDirectory="~/umbraco/uintra" installerFileName="Uintra.Installer.dll" />
  </Actions>

  <control />

  <files>
    <file>
      <guid>Uintra.Installer.dll</guid>
      <orgPath>~/bin</orgPath>
      <orgName>Uintra.Installer.dll</orgName>
    </file>
    <file>
      <guid>Microsoft.Web.XmlTransform.dll</guid>
      <orgPath>~/bin</orgPath>
      <orgName>Microsoft.Web.XmlTransform.dll</orgName>
    </file>
    <file>
      <guid>Compent.Uintra.dll</guid>
      <orgPath>~/Umbraco/uintra</orgPath>
      <orgName>Compent.Uintra.dll</orgName>
    </file>
    <file>
      <guid>App_Plugins.Web.config</guid>
      <orgPath>~/App_Plugins</orgPath>
      <orgName>Web.config</orgName>
    </file>
    <file>
      <guid>Views.Web.config.install.xdt</guid>
      <orgPath>~/Umbraco/uintra</orgPath>
      <orgName>Views.Web.config.install.xdt</orgName>
    </file>
    <file>
      <guid>Web.config.install.xdt</guid>
      <orgPath>~/Umbraco/uintra</orgPath>
      <orgName>Web.config.install.xdt</orgName>
    </file>
  </files>

</umbPackage>