<?xml version="1.0" encoding="utf-8"?>
<umbPackage>
  <info>
    <package>
      <name>Uintra</name>
      <version>0.5.0</version>
      <license url="https://uintra.com/pricing/">Read more</license>
      <url>https://uintra.com/</url>
      <requirements>
        <major>7</major>
        <minor>10</minor>
        <patch>4</patch>
      </requirements>
    </package>
    <author>
      <name>Compent</name>
      <website>https://compent.net/</website>
    </author>
    <readme><![CDATA[Thank you for installing Uintra]]></readme>
  </info>

  <DocumentTypes />
  <Templates />
  <Stylesheets />
  <Macros />
  <DictionaryItems />
  <Languages />
  <DataTypes />

  <Actions>
    <Action runat="install" undo="false" alias="CreateXmlLogFile" version="0.5.0.0" />
    
    <Action runat="install" undo="false" alias="DownloadNugetPackages" uri="https://www.nuget.org/api/v2/package/" path="~/umbraco/uintra/nupkgs/" >
      <Package name="Compent.EnumLists" version="1.1.2" />
      <Package name="Compent.CommandBus" version="1.2.1" />
      <Package name="Compent.Localization.Core" version="0.1.1" />
      <Package name="Compent.Localization.Storage.UDictionary" version="0.1.1" />
      <Package name="Compent.Localization.Umbraco" version="0.1.2" />
      <Package name="Compent.Localization.Web" version="0.0.3" />
      <Package name="Compent.Extensions" version="0.3.4" />
      <Package name="Compent.LinkPreview.HttpClient" version="1.2.6" />
      
      <Package name="EmailWorker.Data" version="0.4.2" />
      <Package name="EmailWorker.Ninject" version="0.0.4" />
      <Package name="EmailWorker.Web" version="0.4.2" />
      
      <Package name="elmah.corelibrary" version="1.2.2" />
      <Package name="elmah" version="1.2.2" />
      <Package name="Elmah.Contrib.WebApi" version="1.0.10.25" />
      <Package name="Elmah.MVC" version="2.0.0" />
      <Package name="uIntra" version="0.5.0" />
      <Package name="Archetype" version="1.10.0" />
      <Package name="Castle.Core" version="4.2.0" />
      <Package name="Elasticsearch.Net" version="5.4.0" />
      <Package name="EntityFramework" version="6.1.3" />
      <Package name="MediaToolkit" version="1.1.0.1" />
      <Package name="System.ValueTuple" version="4.5.0" />
      <Package name="NEST" version="5.3.1" />
      <Package name="System.Runtime.InteropServices.RuntimeInformation" version="4.3.0" />
      
      <Package name="Ninject" version="3.3.3" />
      <Package name="ninject.extensions.conventions" version="3.3.0" />
      <Package name="Ninject.Extensions.Factory" version="3.3.2" />
      <Package name="Ninject.MVC5" version="3.3.0" />
      <Package name="Ninject.Web.Common" version="3.3.0" />
      <Package name="Ninject.Web.Common.WebHost" version="3.3.0" />
      <Package name="WebActivatorEx" version="2.2.0" />
      <Package name="WebActivator" version="1.5.1" />
      <Package name="FluentScheduler" version="5.3.0" />
      <Package name="LanguageExt.Core" version="3.0.28" />
      
      <Package name="uIntra.BaseControls" version="2.6.0" />
      <Package name="uIntra.Bulletins" version="0.5.0" />
      <Package name="uIntra.CentralFeed" version="1.6.0" />
